<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<context-param>
		<param-name>ContextVal</param-name>
		<param-value>2000</param-value>
	</context-param>
	<servlet>
		<servlet-name>Display</servlet-name>
		<jsp-file>/Display.jsp</jsp-file>
		<init-param>
			<param-name>ConfigVal</param-name>
			<param-value>3000</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>Display</servlet-name>
		<url-pattern>/dis</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>input.html</welcome-file>
	</welcome-file-list>
</web-app>